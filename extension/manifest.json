{
  "manifest_version": 2,
  "name": "ExampleFableExtension",
  "version": "0.1",

  "description": "Example extension using F# and Fable.",
  "icons": {
    "48": "icons/icon-48.png"
  },

  "permissions": [ "activeTab" ],

  "browser_specific_settings": {
    "gecko": {
      "id": "@example-fable-extension",
      "data_collection_permissions": {
        "required": [ "none" ]
      }
    }
  },

  "browser_action": {
    "default_icon": "icons/icon-32.png",
    "default_title": "ExampleFableExtension",
    "default_popup": "popup.html"
  }
}